    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.11.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
